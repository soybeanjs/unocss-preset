{
  "name": "@soybeanjs/unocss-preset",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@10.12.4",
  "description": "soybeanjs's unocss preset",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/unocss-preset",
  "repository": {
    "url": "https://github.com/soybeanjs/unocss-preset.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/unocss-preset/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "tsdown",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "@unocss/core": "66.3.3"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.3.1",
    "@soybeanjs/eslint-config": "1.7.0",
    "@types/node": "24.0.10",
    "eslint": "9.30.1",
    "lint-staged": "16.1.2",
    "simple-git-hooks": "2.13.0",
    "tsdown": "0.12.9",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
